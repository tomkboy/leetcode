// search insert position.cpp : 定义控制台应用程序的入口点。
//

#include "stdafx.h"
#include<iostream>

using namespace std;

int search(int A[],int n,int target)
{
	//for(int i=0;i<n;i++)
	//{
	//	if(A[i]<target)
	//	{
	//		if(i)
	//	}//return i+1;
	//}
	if(A[0]>=target)
		return 0;
	int i=0;
	while(i<n&&A[i]<target){++i;}
	return i;
}
int main()
{
	int A[]={1,3,5,7};
	int temp=search(A,4,8);
	cout<<"the insert pos is:"<<temp<<endl;

}

int _tmain(int argc, _TCHAR* argv[])
{
	return 0;
}

